extends layout

block content
  h1= title
  p Welcome to #{title}
  form(method="post" action="/results")
    select(name="number")
      option(value="3") 3
      option(value="4") 4
      option(value="5") 5
      option(value="10") 10
      option(value="20") 20
    input(type="submit" text="Submit")